<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="id.ac.ukdw.tiketonline.DataPenumpangController">
   <children>
      <Pane prefHeight="100.0" prefWidth="600.0" style="-fx-background-color: #f65a00;">
         <children>
            <ImageView fitHeight="100.0" fitWidth="113.0" layoutX="248.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../gambar/Capture.PNG" />
               </image>
            </ImageView>
            <Button fx:id="back" layoutX="14.0" layoutY="35.0" mnemonicParsing="false" onAction="#heandleback" prefHeight="31.0" prefWidth="84.0" style="-fx-background-radius: 40 0 0 40; -fx-background-color: #000000;" text="Back" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
         </children>
      </Pane>
      <TextField fx:id="namaDepan" layoutX="92.0" layoutY="187.0" promptText="nama depan" />
      <TextField fx:id="namaBelakang" layoutX="361.0" layoutY="187.0" promptText="nama belakang" />
      <TextField fx:id="email" layoutX="361.0" layoutY="242.0" promptText="Alamat email" />
      <TextField fx:id="nKTP" layoutX="92.0" layoutY="242.0" promptText="Nomor KTP" />
      <ComboBox fx:id="title" layoutX="93.0" layoutY="138.0" prefWidth="150.0" />
      <Button fx:id="next" layoutX="245.0" layoutY="341.0" mnemonicParsing="false" onAction="#hendleContinue" prefHeight="31.0" prefWidth="111.0" style="-fx-background-color: f65a00;" text="Continue" textFill="#cdcdcd">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
